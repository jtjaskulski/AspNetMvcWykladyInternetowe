@model IEnumerable<Company.Data.Data.CMS.Page>

<div class="nav-wrapper container">
    <a id="logo-container" href="#" class="brand-logo">WSB-NLU MVC</a>
    <ul class="right hide-on-med-and-down">
        @foreach (var item in Model)
        { 
            <li>@Html.ActionLink(item.LinkTitle, "Index", "Home", new { id = item.IdPage})</li>
        }
        <li>@Html.ActionLink("Strona główna", "Index", "Home")</li>
        <li>@Html.ActionLink("O firmie", "About", "Home")</li>
        <li>@Html.ActionLink("Kontakt", "Contact", "Home")</li>
    </ul>

    <ul id="nav-mobile" class="sidenav">
        @foreach (var item in Model)
        {
            <li>@Html.ActionLink(item.LinkTitle, "Index", "Home", new { id = item.IdPage })</li>
        }
        <li>@Html.ActionLink("Strona główna", "Index", "Home")</li>
        <li>@Html.ActionLink("O firmie", "About", "Home")</li>
        <li>@Html.ActionLink("Kontakt", "Contact", "Home")</li>
    </ul>
    <a href="#" data-target="nav-mobile" class="sidenav-trigger"><i class="material-icons">menu</i></a>
</div>