@model IEnumerable<Company.Data.Data.Shop.TypeOfProduct>
<div class="nav-wrapper container">
    <a id="logo-container" href="#" class="brand-logo">WSB-NLU MVC</a>
    <ul class="right hide-on-med-and-down">
        @foreach (var item in Model)
        {
            <li>@Html.ActionLink(item.Name, "Index", "Shop", new { id = item.IdTypeOfProduct }, null)</li>
        }
        <li>@Html.ActionLink("Strona główna", "Index", "Home")</li>
        <li>@Html.ActionLink("Promocje", "Discounts", "Home")</li>
        <li>
            <a class="nav-link text-dark" asp-controller="Cart" asp-action="Index">Koszyk</a>
        </li>
    </ul>

    <ul id="nav-mobile" class="sidenav">
        @foreach (var item in Model)
        {
            <li>@Html.ActionLink(item.Name, "Index", "Shop", new { id = item.IdTypeOfProduct }, null)</li>
        }
        <li>@Html.ActionLink("Strona główna", "Index", "Home")</li>
        <li>@Html.ActionLink("Promocje", "Discounts", "Shop")</li>
        <li>
            <a class="nav-link text-dark" asp-controller="Cart" asp-action="Index">Koszyk</a>
        </li>
    </ul>
    <a href="#" data-target="nav-mobile" class="sidenav-trigger"><i class="material-icons">menu</i></a>
</div>